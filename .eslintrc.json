{
  "env": {
    "browser": true,
    "es6": true,
    "amd": true,
    "commonjs": true
  },
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "extends": "eslint:recommended",
  "globals": {
    "process": "readonly"
  },
  "rules": {
    "indent": ["error", 2],
    "quotes": ["error", "single"],
    "no-extra-semi": "off"
  }
}
